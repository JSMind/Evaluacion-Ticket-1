                    <button >NUEVO PRESUPUESTO</button>
                    <button>EDITAR</button>
                    <button>ELIMINAR</button>
                    <button>ENVIAR</button>
                    <thead>
                    <tr class="table-info">
                        <th scope="col">ID PRESUPUESTO</th>
                        <th scope="col">FECHA CREACION</th>
                        <th scope="col">PROYECTO</th>
                        <th scope="col">VERSIONES</th>
                    </tr>
                    </thead>
                    <tbody id="items"></tbody>
                </table>
            </div>
            <br>
            <br>      
            <!-- Template que se utiliza para mostrar los Datos de la Tabla Presupuestos -->
            <template id="template-presupuestos">
